                android:layout_weight="1"
                android:text="@string/m"
                android:textColor="@android:color/black"
                android:textSize="8sp" />
        </LinearLayout>

        <com.truckspot.utils.ELDGraph
            android:id="@+id/graph"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginStart="24dp"
            android:layout_marginTop="14dp"
            android:layout_marginEnd="20dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/violation"
            android:layout_width="5dp"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/graph"
            android:layout_alignTop="@+id/graph"
            android:layout_alignBottom="@+id/graph"
            android:background="#55ff0000"
            android:visibility="gone" />

        <LinearLayout
            android:id="@+id/hours"
            android:layout_width="20dp"
            android:layout_height="match_parent"
            android:layout_alignParentEnd="true"
            android:orientation="vertical"
            android:weightSum="4">

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/off_duty_hours"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginTop="15dp"
                android:layout_weight="1"
                android:gravity="center"
                android:text="@string/_0"
                android:textColor="@android:color/black"
                android:textSize="8sp" />

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/sb_hours"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:gravity="center"
                android:text="@string/_0"
                android:textColor="@android:color/black"
                android:textSize="8sp" />

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/driving_hours"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:gravity="center"
                android:text="@string/_0"
                android:textColor="@android:color/black"
                android:textSize="8sp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:orientation="vertical">

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/on_duty_hours"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:lineSpacingExtra="0.1dp"
                    android:text="@string/_0"
                    android:textColor="@android:color/black"
                    android:textSize="8sp" />

                <View
                    android:layout_width="12dp"
                    android:layout_height="1dp"
                    android:layout_gravity="center_horizontal"
                    android:background="@android:color/holo_blue_dark" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/total_hours"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:lineSpacingExtra="0.1dp"
                    android:text="@string/_0"
                    android:textColor="@android:color/black"
                    android:textSize="8sp" />
            </LinearLayout>
        </LinearLayout>
